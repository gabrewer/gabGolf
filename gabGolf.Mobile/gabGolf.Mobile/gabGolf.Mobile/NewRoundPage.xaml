<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="gabGolf.Mobile.NewRoundPage"
             Title="NewRoundPage">
  <ScrollView>
    <StackLayout Padding="10">
            <Label Text="New Round" FontSize="40"/>
            <Label Text="Course Name"></Label>
            <Editor Text="{Binding CourseName}"></Editor>
            <Label Text="Date"></Label>
            <DatePicker Date="{Binding DatePlayed}"></DatePicker>
            <Button Text="Start Round" Command="{Binding StartRoundCommand}"></Button>
        </StackLayout>
  </ScrollView>
</ContentPage>
    